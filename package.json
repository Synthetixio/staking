		],
		"moduleDirectories": [
			"node_modules"
		],
		"globalSetup": "./test-setup/global.js",
		"setupFilesAfterEnv": [
			"./test-setup/setup.js"
		],
		"testEnvironment": "jest-environment-jsdom",
		"transform": {
			"\\.svg$": "svg-jest",
			"\\.[jt]sx?$": "babel-jest"
		},
		"moduleNameMapper": {
